{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "standard-with-typescript",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["warn", 2],
    "semi": ["warn", "always"],
    "quotes": ["warn", "double"],
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-useless-escape": "warn",
    "no-useless-constructor": "warn",
    "no-useless-return": "warn",
    "no-useless-catch": "warn"
  },
  "settings": {

  }
}
